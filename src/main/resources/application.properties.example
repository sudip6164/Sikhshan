spring.application.name=sikhshan-backend

spring.datasource.driver-class-name=com.mysql.jdbc.Driver
spring.datasource.url=jdbc:mysql://localhost:3306/database_db?createDatabaseIfNotExist=true # instead of database write your own database name
spring.datasource.username=root
spring.datasource.password=

spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true

server.port= # port number

jwt.secret=PASTE_YOUR_BASE64_SECRET_HERE
jwt.expiration=3600000 # 1 hour in ms
